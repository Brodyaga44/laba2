<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="474.0" prefWidth="696.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.imagetest.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>

  <Label fx:id="welcomeText" />
   <ImageView fx:id="imageView" fitHeight="261.0" fitWidth="573.0" pickOnBounds="true" preserveRatio="true" />
   <AnchorPane prefHeight="48.0" prefWidth="591.0">
      <children>
        <Button layoutX="348.0" layoutY="11.0" onAction="#onHelloButtonClick" text="Слайдшоу" />
         <Button layoutX="200.0" layoutY="11.0" mnemonicParsing="false" onAction="#onNextClick" text="Дальше" />
         <Button layoutX="118.0" layoutY="11.0" mnemonicParsing="false" onAction="#onLastClick" text="Назад" />
         <Button layoutX="458.0" layoutY="11.0" mnemonicParsing="false" onAction="#onStopClick" text="Стоп слайдшоу" />
      </children>
   </AnchorPane>
</VBox>
